package springIntro_BTK;

public class Main {

	public static void main(String[] args) {
		
//System.out.println("Musa");
Musa musa = new Musa();
musa.id=5;
System.out.println(musa.id);
	}

	
}
class Musa
{
	public int id;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	}